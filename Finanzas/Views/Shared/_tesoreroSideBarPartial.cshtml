@using Finanzas.Models
<div class="leftpanel">

            <div class="logopanel">
                <h1><span>[</span> IglesiAlDia <span>]</span></h1>
            </div>
            <!-- logopanel -->

            <div class="leftpanelinner">

                <div class="visible-xs hidden-sm hidden-md hidden-lg">
                    <div class="media userlogged">
                        <div class="media-body">
                            @{
                                var user = Session["usuarioActual"] as Usuario;
                            }
                            <h4>@user.nombreUsuario (@user.Rol.nombreRol)</h4>
                        </div>
                    </div>

                    <h5 class="sidebartitle actitle">Cuenta</h5>
                    <ul class="nav nav-pills nav-stacked nav-bracket mb30">
                        <li><a href="/Account/ChangePassword"><i class="fa fa-cog"></i><span>Cambiar Contraseña</span></a></li>
                        <li><a href="/Account/Logoff"><i class="fa fa-sign-out"></i><span>Cerrar Seccion</span></a></li>
                    </ul>
                </div>

                <h5 class="sidebartitle">Navigation</h5>
                <ul class="nav nav-pills nav-stacked nav-bracket">
                    <li class="active"><a href="/"><i class="fa fa-home"></i><span>Inicio</span></a></li>
                    <li><a href="/Cuenta/"><i class="fa fa-envelope-o"></i><span>Cuentas</span></a></li>
                    <li class="nav-parent"><a href="#"><i class="fa fa-suitcase"></i><span>Finanzas de Iglesia</span></a>
                        <ul class="children">
                            <li><a href="/Diezmo/"><i class="fa fa-caret-right"></i>Diezmos</a></li>
                            <li><a href="/IngresoFinanza/"><i class="fa fa-caret-right"></i>Ingresos</a></li>
                            <li><a href="/GastoFinanza/"><i class="fa fa-caret-right"></i>Gastos</a></li>
                        </ul>
                    </li>
                    <li class="nav-parent"><a href="#"><i class="fa fa-file-text"></i><span>Comedor</span></a>
                        <ul class="children">
                            <li><a href="/Producto/"><i class="fa fa-caret-right"></i>Producto</a></li>
                            <li><a href="/Almacen/"><i class="fa fa-caret-right"></i>Almacen</a></li>
                            <li><a href="/CompraProducto/"><i class="fa fa-caret-right"></i>Compras</a></li>
                            <li><a href="/SalidaAlmacen/"><i class="fa fa-caret-right"></i>Salidas de Almacen</a></li>
                        </ul>
                    </li>
                    <li><a href="/Reporte/"><i class="fa fa-laptop"></i><span>Reportes</span></a></li>
                </ul>

                <div class="infosummary">
                    <h5 class="sidebartitle">Information Summary</h5>
                    <ul>
                        <li>
                            <div class="datainfo">
                                <span class="text-muted">Daily Traffic</span>
                                <h4>630, 201</h4>
                            </div>
                            <div id="sidebar-chart" class="chart"></div>
                        </li>
                        <li>
                            <div class="datainfo">
                                <span class="text-muted">Average Users</span>
                                <h4>1, 332, 801</h4>
                            </div>
                            <div id="sidebar-chart2" class="chart"></div>
                        </li>
                        <li>
                            <div class="datainfo">
                                <span class="text-muted">Disk Usage</span>
                                <h4>82.2%</h4>
                            </div>
                            <div id="sidebar-chart3" class="chart"></div>
                        </li>
                        <li>
                            <div class="datainfo">
                                <span class="text-muted">CPU Usage</span>
                                <h4>140.05 - 32</h4>
                            </div>
                            <div id="sidebar-chart4" class="chart"></div>
                        </li>
                        <li>
                            <div class="datainfo">
                                <span class="text-muted">Memory Usage</span>
                                <h4>32.2%</h4>
                            </div>
                            <div id="sidebar-chart5" class="chart"></div>
                        </li>
                    </ul>
                </div>
                <!-- infosummary -->

            </div>
            <!-- leftpanelinner -->
        </div>
        <!-- leftpanel -->